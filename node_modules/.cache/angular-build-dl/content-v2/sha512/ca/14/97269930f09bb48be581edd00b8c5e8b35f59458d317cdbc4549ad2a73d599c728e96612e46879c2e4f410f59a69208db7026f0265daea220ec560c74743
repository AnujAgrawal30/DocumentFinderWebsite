            params: {
              id: id.toString()
            }
          });
        }
      }, {
        key: "test",
        value: function test() {
          this.http.get(this.BASE_URL).subscribe(function (data) {
            console.log(data);
          });
        }
      }, {
        key: "addTag",
        value: function addTag(tag, filepath) {
