        this.BASE_URL = "http://127.0.0.1:8000";
      }

      _createClass(NetworkCallsService, [{
        key: "getSearchResults",
        value: function getSearchResults(query, method) {
          return this.http.get(this.BASE_URL + '/search', {
            params: {
              query: query,
              method: method
            }
          });
        }
      }, {
        key: "addTrainPath",
        value: function addTrainPath(path) {
